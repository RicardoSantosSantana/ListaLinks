{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node" ,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/*.ts"
, "src/pages/api/auth/[...nextauth].js", "src/pages/api/auth/signin.jsx", "src/pages/api/users.js", "src/db/config/config.js", "src/pages/error.js", "src/pages/middleware.tse.js", "src/pages/app/error.jsx" ],
  "exclude": [
    "node_modules"
  ]
}
